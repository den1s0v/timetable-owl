<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://vstu.ru/poas/s09/timetable"
         xmlns="http://vstu.ru/poas/s09/timetable#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#">

<owl:Ontology rdf:about="http://vstu.ru/poas/s09/timetable"/>

<owl:ObjectProperty rdf:about="#isPartOfTimetable">
  <rdfs:domain rdf:resource="#TimetableElement"/>
  <rdfs:range rdf:resource="#Timetable"/>
  <rdfs:subPropertyOf rdf:resource="#hasPart"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#takesPlace">
  <rdfs:domain rdf:resource="#Lesson"/>
  <rdfs:range rdf:resource="#Room"/>
  <rdfs:subPropertyOf rdf:resource="#referencesTo"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isAtTimeslot">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
  <rdfs:domain rdf:resource="#Lesson"/>
  <rdfs:range rdf:resource="#Timeslot"/>
  <rdfs:subPropertyOf rdf:resource="#referencesTo"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#realizes">
  <rdfs:domain rdf:resource="#Lesson"/>
  <rdfs:range rdf:resource="#SubjAssignment"/>
  <rdfs:subPropertyOf rdf:resource="#referencesTo"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasLearningAssignment">
  <rdfs:domain rdf:resource="#Group"/>
  <rdfs:range rdf:resource="#SubjAssignment"/>
  <rdfs:subPropertyOf rdf:resource="#referencedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasTeachingAssignment">
  <rdfs:domain rdf:resource="#Professor"/>
  <rdfs:range rdf:resource="#SubjAssignment"/>
  <rdfs:subPropertyOf rdf:resource="#referencedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasSubject">
  <rdfs:domain rdf:resource="#SubjAssignment"/>
  <rdfs:range rdf:resource="#Subject"/>
  <rdfs:subPropertyOf rdf:resource="#referencesTo"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasUniqueData">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#weekDays">
  <rdfs:domain rdf:resource="#Timetable"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:subPropertyOf rdf:resource="#hasUniqueData"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#dayHours">
  <rdfs:domain rdf:resource="#Timetable"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:subPropertyOf rdf:resource="#hasUniqueData"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#capacity">
  <rdfs:domain rdf:resource="#Room"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:subPropertyOf rdf:resource="#hasUniqueData"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#day">
  <rdfs:domain rdf:resource="#Timeslot"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:subPropertyOf rdf:resource="#hasUniqueData"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hour">
  <rdfs:domain rdf:resource="#Timeslot"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:subPropertyOf rdf:resource="#hasUniqueData"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#size">
  <rdfs:domain rdf:resource="#Group"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:subPropertyOf rdf:resource="#hasUniqueData"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#name">
  <rdfs:subPropertyOf rdf:resource="#hasUniqueData"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hours">
  <rdfs:domain rdf:resource="#SubjAssignment"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:subPropertyOf rdf:resource="#hasUniqueData"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasError">
  <rdfs:domain rdf:resource="#TimetableError"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Timetable">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#TimetableElement">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Room">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Lesson">
  <rdfs:subClassOf rdf:resource="#TimetableElement"/>
</owl:Class>

<owl:Class rdf:about="#Timeslot">
  <rdfs:subClassOf rdf:resource="#TimetableElement"/>
</owl:Class>

<owl:Class rdf:about="#Group">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Subject">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#SubjAssignment">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Professor">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#referencesTo">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
</owl:Class>

<owl:Class rdf:about="#referencedBy">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
</owl:Class>

<owl:Class rdf:about="#hasPart">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
</owl:Class>

<owl:Class rdf:about="#TimetableError">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<TimetableError rdf:about="#ERRORS">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</TimetableError>

<swrl:Variable rdf:about="urn:swrl#r"/>

<swrl:Variable rdf:about="urn:swrl#l"/>

<swrl:Variable rdf:about="urn:swrl#sa"/>

<swrl:Variable rdf:about="urn:swrl#g"/>

<swrl:Variable rdf:about="urn:swrl#cap"/>

<swrl:Variable rdf:about="urn:swrl#n"/>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#Room"/>
          <swrl:argument1 rdf:resource="urn:swrl#r"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:ClassAtom>
              <swrl:classPredicate rdf:resource="#Lesson"/>
              <swrl:argument1 rdf:resource="urn:swrl#l"/>
            </swrl:ClassAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:ClassAtom>
                  <swrl:classPredicate rdf:resource="#SubjAssignment"/>
                  <swrl:argument1 rdf:resource="urn:swrl#sa"/>
                </swrl:ClassAtom>
              </rdf:first>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first>
                    <swrl:ClassAtom>
                      <swrl:classPredicate rdf:resource="#Group"/>
                      <swrl:argument1 rdf:resource="urn:swrl#g"/>
                    </swrl:ClassAtom>
                  </rdf:first>
                  <rdf:rest>
                    <rdf:Description>
                      <rdf:first>
                        <swrl:DatavaluedPropertyAtom>
                          <swrl:propertyPredicate rdf:resource="#capacity"/>
                          <swrl:argument1 rdf:resource="urn:swrl#r"/>
                          <swrl:argument2 rdf:resource="urn:swrl#cap"/>
                        </swrl:DatavaluedPropertyAtom>
                      </rdf:first>
                      <rdf:rest>
                        <rdf:Description>
                          <rdf:first>
                            <swrl:DatavaluedPropertyAtom>
                              <swrl:propertyPredicate rdf:resource="#size"/>
                              <swrl:argument1 rdf:resource="urn:swrl#g"/>
                              <swrl:argument2 rdf:resource="urn:swrl#n"/>
                            </swrl:DatavaluedPropertyAtom>
                          </rdf:first>
                          <rdf:rest>
                            <rdf:Description>
                              <rdf:first>
                                <swrl:IndividualPropertyAtom>
                                  <swrl:propertyPredicate rdf:resource="#hasLearningAssignment"/>
                                  <swrl:argument1 rdf:resource="urn:swrl#g"/>
                                  <swrl:argument2 rdf:resource="urn:swrl#sa"/>
                                </swrl:IndividualPropertyAtom>
                              </rdf:first>
                              <rdf:rest>
                                <rdf:Description>
                                  <rdf:first>
                                    <swrl:IndividualPropertyAtom>
                                      <swrl:propertyPredicate rdf:resource="#realizes"/>
                                      <swrl:argument1 rdf:resource="urn:swrl#l"/>
                                      <swrl:argument2 rdf:resource="urn:swrl#sa"/>
                                    </swrl:IndividualPropertyAtom>
                                  </rdf:first>
                                  <rdf:rest>
                                    <rdf:Description>
                                      <rdf:first>
                                        <swrl:IndividualPropertyAtom>
                                          <swrl:propertyPredicate rdf:resource="#takesPlace"/>
                                          <swrl:argument1 rdf:resource="urn:swrl#l"/>
                                          <swrl:argument2 rdf:resource="urn:swrl#r"/>
                                        </swrl:IndividualPropertyAtom>
                                      </rdf:first>
                                      <rdf:rest>
                                        <rdf:Description>
                                          <rdf:first>
                                            <swrl:BuiltinAtom>
                                              <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#lessThan"/>
                                              <swrl:arguments>
                                                <rdf:Description>
                                                  <rdf:first rdf:resource="urn:swrl#cap"/>
                                                  <rdf:rest>
                                                    <rdf:Description>
                                                      <rdf:first rdf:resource="urn:swrl#n"/>
                                                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                    </rdf:Description>
                                                  </rdf:rest>
                                                </rdf:Description>
                                              </swrl:arguments>
                                            </swrl:BuiltinAtom>
                                          </rdf:first>
                                          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                        </rdf:Description>
                                      </rdf:rest>
                                    </rdf:Description>
                                  </rdf:rest>
                                </rdf:Description>
                              </rdf:rest>
                            </rdf:Description>
                          </rdf:rest>
                        </rdf:Description>
                      </rdf:rest>
                    </rdf:Description>
                  </rdf:rest>
                </rdf:Description>
              </rdf:rest>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:DatavaluedPropertyAtom>
          <swrl:propertyPredicate rdf:resource="#hasError"/>
          <swrl:argument1 rdf:resource="#ERRORS"/>
          <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Room overflow</swrl:argument2>
        </swrl:DatavaluedPropertyAtom>
      </rdf:first>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>


</rdf:RDF>
